<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="nemesis.diiscover.laboratorioPantalla"
    android:background="#ffffff">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/computer2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Piso"
        android:id="@+id/textView4"
        android:layout_marginTop="54dp"
        android:layout_alignParentStart="true"
        android:textColor="#010101"
        android:textSize="20dp"
        android:layout_below="@+id/imageView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Piso no definido"
        android:id="@+id/textPiso"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:textSize="20dp" />

    <view
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:id="@+id/view3"
        android:layout_below="@+id/textPiso"
        android:background="#757575"
        android:visibility="visible"
        android:layout_marginTop="7dp"
        class="View" />

    <view
        class="View"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:id="@+id/view5"
        android:background="#757575"
        android:visibility="visible"
        android:layout_below="@+id/textView3"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ordenadores Libres"
        android:id="@+id/textView8"
        android:textColor="#010101"
        android:textSize="20dp"
        android:layout_below="@+id/view5"
        android:layout_alignParentStart="true"
        android:layout_marginTop="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Desconocido"
        android:textSize="20dp"
        android:id="@+id/textPcLibre"
        android:layout_below="@+id/view5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="7dp" />

    <view
        class="View"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:id="@+id/view4"
        android:background="#757575"
        android:visibility="visible"
        android:layout_below="@+id/textView8"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Número"
        android:id="@+id/textView3"
        android:layout_below="@+id/view3"
        android:layout_alignParentStart="true"
        android:textSize="20dp"
        android:layout_marginTop="7dp"
        android:textColor="#010101" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Número desconocido"
        android:id="@+id/textNumero"
        android:layout_below="@+id/view3"
        android:layout_alignParentEnd="true"
        android:textSize="20dp"
        android:layout_marginTop="7dp" />
</RelativeLayout>
